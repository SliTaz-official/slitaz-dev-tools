{header}
<title>{repo|escape}: log</title>
<link rel="alternate" type="application/atom+xml"
   href="{url}atom-log" title="Atom feed for {repo|escape}" />
<link rel="alternate" type="application/rss+xml"
   href="{url}rss-log" title="RSS feed for {repo|escape}" />
</head>
<body>

<!-- Header -->
<div id="header">
	<div id="logo"></div>
	<div id="network">
		<a href="http://www.slitaz.org/">Home</a>
		<a href="http://scn.slitaz.org/">Community</a>
		<a href="http://doc.slitaz.org/">Doc</a>
		<a href="http://forum.slitaz.org/">Forum</a>
		<a href="http://slitaz.pro/">Pro</a>
		<a href="http://bugs.slitaz.org">Bugs</a>
		<a href="http://hg.slitaz.org/">Hg</a>
		<a href="http://cook.slitaz.org/">Cook</a>
	</div>
	<h1><a href="http://hg.slitaz.org/">SliTaz Repositories</a></h1>
</div>

<!-- Block -->
<div id="block">
	<!-- Navigation -->
	<div id="block_nav">
		<h4>Navigation</h4>
		<div class="right_box">
			<ul>
				<li><a href="{url}rev/{node|short}{sessionvars%urlparameter}">Changeset</a></li>
				<li><a href="{url}file/{node|short}{path|urlescape}{sessionvars%urlparameter}">Browse</a></li>
			</ul>
		</div>
		<div class="left_box">
			<ul>
				<li><a href="{url}summary{sessionvars%urlparameter}">Summary</a></li>
				<li>Changelog</li>
				<li><a href="{url}graph/{node|short}{sessionvars%urlparameter}">Graph</a></li>
				<li><a href="{url}tags{sessionvars%urlparameter}">Tags</a></li>
				<li><a href="{url}branches{sessionvars%urlparameter}">Branches</a></li>
			</ul>
		</div>
	</div>
	<!-- Information/image -->
	<div id="block_info">
		<h4>Project: {repo|escape}</h4>
		<p>
			Clone project: hg clone http://hg.slitaz.org/{repo|escape}
		</p>
		<div id="tarball">
			<ul>
				<li>Download tarball:</li>
				{archives%archiveentry}
			</ul>
		</div>
		<h4>Search</h4>
		<form class="search" action="{url}log">
			{sessionvars%hiddenformentry}
			<p><input name="rev" id="search1" type="text" /></p>
			<!-- <div id="hint">find changesets by author, revision,
			files, or words in the commit message</div> -->
		</form>
	</div>
</div>

<!-- Content -->
<div id="content">

<h2><a href="{url}{sessionvars%urlparameter}">{repo|escape}</a> log</h2>

<div class="navigate">rev {rev}: {changenav%navshort}</div>

<table class="bigtable">
 <tr>
  <th class="age">age</th>
  <th class="author">author</th>
  <th class="description">description</th>
 </tr>
{entries%shortlogentry}
</table>

<div class="navigate">rev {rev}: {changenav%navshort}</div>

<!-- End of content -->
</div>

{footer}
